sudo updatedb
locate libcjson.so
/usr/lib/x86_64-linux-gnu/libcjson.so
export LD_LIBRARY_PATH=/path/to/cjson/library:$LD_LIBRARY_PATH


cc authserver.c -o authserver -lcjson -L/usr/lib/x86_64-linux-gnu/